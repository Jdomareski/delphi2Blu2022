{$R *.dfm}

procedure TForm1.Button1Click(Sender: TObject);
var
xNum1 : Integer;
xNum2 : integer;

begin
xNum1 := strToInt(inputbox('PRIMEIRO NÚMERO', 'Insira o primeiro numero', ''));
xNum2 := strToInt(inputbox('SEGUNDO NÚMERO', 'Insira o segundo numero', ''));

  if xNum1 = xNum2 then
      showMessage('Os Números São Iguais')
  else if xNum1 <> xNum2 then
    begin
      showMessage('Os Números não São Iguais');
      if xNum1 > xNum2 then
    end;



end;

end.
